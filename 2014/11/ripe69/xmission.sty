\ProvidesPackage{xmission}
\RequirePackage{circuitikz}

\def\xmissionline{
  \node[ocirc] at (0,1) {};
  \node[ocirc] at (0,-1) {};
  \node[ocirc] at (10,1) {};
  \node[ocirc] at (10,-1) {};
  \draw (0,1) to (1,1);
  \draw (1,1) to[R, l=$R$] (3,1);
  \draw (3,1) to[L, l=$L$] (6,1);
  \draw (6,1) to (10,1);
  \draw (6.5,-1) to[R, *-*, l=$G$] (6.5,1);
  \draw (8,1) to[C, *-*, l=$C$] (8,-1);
  \draw (0,-1) to (10,-1);
}
\def\telegrapher{\gamma = \sqrt{(R + i\omega L)(G + i\omega C)}}
